@import url("https://fonts.googleapis.com/css?family=Beth+Ellen|Libre+Franklin&display=swap");
html {
  height: 100%;
  font-size: 16px;
  margin: 0;
  padding: 0;
}
h1 {
  font-family: "Beth Ellen", cursive;
  text-shadow: 1px 1px 10px rgba(0, 0, 0, 0.2);
  font-size: 5vw;
  margin: 1vh;
}
h2 {
  font-family: "Libre Franklin", sans-serif;
  font-size: 3vw;
}
body {
  background: #7aa1bd;
  background: linear-gradient(180deg, #7aa1bd 0%, #00bdff 100%);
  background-attachment: fixed;
  height: 100%;
}
section {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.social-container {
  display: flex;
}
.avatar {
  border: 0.5em solid grey;
  max-height: 40vh;
  max-width: 40vh;
  margin: 5vw;
  border-radius: 50%;
  overflow: hidden;
  box-shadow: 0px 6px 20px 3px rgba(0, 0, 0, 0.2);
}
.avatar img {
  z-index: 0;
  height: 100%;
  width: 100%;
}
ul {
  font-family: "Libre Franklin", sans-serif;
  list-style-type: none;
  padding: 0;
  margin: 0;
}
ul .very-large {
  font-size: 10vh;
}
a.social-icon {
  height: 32px;
  margin: 1em;
}
a.social-icon.github {
  width: 32px;
  background: url(GitHub-Mark-64px.094471cd.HASH_REF_a808a49294dfa24abe0c5e012941e6e5.png);
  background-blend-mode: multiply;
  background-size: contain;
}
a.social-icon.linkedin {
  width: 38px;
  height: 32px;
  background: url(LI-In-Bug.5042f4a7.HASH_REF_06c98f10023bdf871bc0038a15a8afb9.png);
  background-repeat: no-repeat;
  filter: brightness(0);
  background-size: contain;
}

